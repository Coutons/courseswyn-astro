import coupons from "../data/coupons.json";

const allPosts = await fetchPosts();
const featuredPosts = allPosts.slice(0, 3);
const featuredCoupons = coupons.slice(0, 3);

// Dynamic SEO helper functions
function getCurrentMonthYear(): string {
  const now = new Date();
  const options: Intl.DateTimeFormatOptions = { month: 'long', year: 'numeric' };
  return now.toLocaleDateString('en-US', options);
}

function getCouponsAddedInLast24Hours(): number {
  const now = new Date();
  const oneDayAgo = new Date(now.getTime() - 24 * 60 * 60 * 1000);
  
  return coupons.filter(coupon => {
    const addedDate = new Date(coupon.addedDate);
    return addedDate >= oneDayAgo && addedDate <= now;
  }).length;
}

const totalCoupons = coupons.length;
const currentMonthYear = getCurrentMonthYear();
const couponsLast24h = getCouponsAddedInLast24Hours();

